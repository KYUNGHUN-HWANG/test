      FR_SRLR_OpDone      , 16;
      FR_SRLR_VddReset    , 17;
      FR_SRLR_Steploss    , 18;
      FR_SRLR_EIDef       , 19;
      FR_SRLR_TSD         , 20;
      FR_SRLR_TW          , 21;
