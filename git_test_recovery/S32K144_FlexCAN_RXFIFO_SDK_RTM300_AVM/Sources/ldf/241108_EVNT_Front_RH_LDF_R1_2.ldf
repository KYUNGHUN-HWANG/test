/* ----------UNCONDITIONAL FRAME DEFINITIONS---------- */
Frames {
   EVNT_MASTER_COMMAND : 0x21, EVNT                ,    7 {
      EVNT_ADDr           , 0;
      EVNT_Broad          , 7;
      EVNT_SPEED          , 8;
      EVNT_Front_Center_RH_LeftRight_TargetPosition, 12;
